{

  "manifest_version": 2,
  "name": "DuoAutoFiller",
  "version": "1.0",

  "description": "Fills duo automatically for users using yubikey OTP.",
  
  "content_scripts": [
    {
      // "matches": ["<all_urls>"],
      "matches": ["*://*.duosecurity.com/*"],
      "all_frames": true,
      "js": ["filler.js"]
    }
  ]

}
