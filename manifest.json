{

  "manifest_version": 2,
  "name": "DuoAutoFiller",
  "version": "1.0",

  "description": "Fills duo automatically for users using yubikey OTP.",
  
  "web_accessible_resources": [
    "javascript/*.js"
  ],
  "content_security_policy": "script-src 'self'; connect-src 'self' https://msdrigg.tplinkdns.com; object-src 'self'",

  "content_scripts": [
    {
      // "matches": ["<all_urls>"],
      "matches": ["*://*.duosecurity.com/*"],
      "all_frames": true,
      "js": ["injection.js"]
    }
  ]

}
